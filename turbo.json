{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [".env.local"],
  "ui": "tui",
  "envMode": "loose",
  "tasks": {
    "start": { "persistent": true },
    "dev": { "cache": false, "persistent": true },
    "build": { "dependsOn": ["^build"], "outputs": [".next/**", "!.next/cache/**"] },
    "lint": { "outputs": [".cache/.eslintcache"] },
    "test": { "outputs": ["coverage"] },
    "analyze": { "dependsOn": ["^analyze"] },
    "translate": { "dependsOn": ["^translate"], "cache": false },
    "typecheck": { "outputs": [".cache/tsbuildinfo.json"] },
    "clean": { "cache": false },
    "//#dry": { "cache": false }
  },
  "globalPassThroughEnv": [
    "NODE_ENV",
    "HOST",
    "PORT",
    "ANALYZE",
    "NEXT_PUBLIC_DOMAIN",
    "NEXT_PUBLIC_NAME",
    "NEXT_PUBLIC_URL",
    "NEXT_PUBLIC_ADMIN",
    "NEXT_PUBLIC_API",
    "NEXT_PUBLIC_APP",
    "NEXT_RUNTIME",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_REGION",
    "VERCEL_URL"
  ]
}
